          //Test branching, forward and backward
          //Make sure two consecutive branches work
          //Test all types of branches
          //
          
          //									pc in hex
0000      .start:
0000 c10a LBI	r1, 10		//						pc = 0
0002 c205 LBI	r2, 5		//if r2 ever gets 6, something went wrong	pc = 2
          
0004      .loop:			
0004 493f ADDI r1, r1, -1		//decrement
          BEGZ r1, .loop		//Loop
          
0006 c3f8 LBI r3, -8
0008 c414 LBI r4, 20
          
          
000a 8460 ST r3, r4, 0		//store -8 at address 20
000c 8ca0 LD r5, r4, 0		//retrieve value
000e 7504 BLTZ r5, .target1
          
0010 c206 LBI r2, 6		//this should not be executed
0012 0000 HALT			//this should not be executed
          
0014      .target1:
0014 7dea BGEZ r5, .start		//this should not be executed
0016 c308 LBI r3, 8
          
0018 8460 ST r3, r4, 0		
001a 8ca0 LD r5, r4, 0
001c 7d04 BGEZ r5, .target2
          
001e c206 LBI r2, 6		//this should not be executed
0020 0000 HALT			//this should not be executed
          
0022      .target2:
0022 8464 ST r3, r4, 4		//test stall after store, no dependency
0024 7d04 BGEZ r5, .target3
          
0026 c206 LBI r2, 6		//this should not be executed
0028 0000 HALT			//this should not be executed
          
002a      .target3:
          					//exit
002a c301 LBI r3, 1
002c 0000 HALT					
