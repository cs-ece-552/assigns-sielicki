000c      .label1:
000c c3fd lbi r3, -3        //set the r3 if r1 is less than r2, it won't be set here
000e 7304 bltz r3, .label2     //if r3 is not zero, program will branch to label1, and it won't do branch here.
0010 4c84 addi r4, r4, 4	     //if the bltz above didn't branch, r4 will be added 4
0012 0000 halt
          
0014      .label2:
