          // SLT
          // if(Rs < Rt) then Rd <- 1 else Rd <-0
          
          // Load Rs with smaller # than Rt
0000 c60a lbi  r6, 0x0a
0002 960a slbi r6, 0x0a
          // Load Rt with a larger # (not all ones though)
0004 c31a lbi  r3, 0x1a
0006 931a slbi r3, 0x1a
