          // firstTest
          //
          // First (simplest) test program for WISC-SP06 architecture
          // for the single-cycle implementation.
          //
          // CS/ECE 552, Spring 2006
          // Andy Phelps  24 March 2006
          //
          
          // Start with a few simple tests:
          
0000 c103 lbi r1, 3          // r1 = 3
0002 c204 lbi r2, 4          // r2 = 4
0004 da2c add r3, r2, r1     // r3 = 7
          
0006 0800 nop
          
0008 c455 lbi r4, 0x55       // r4 = 0x0055
000a c5aa lbi r5, 0xaa       // r5 = 0xffaa
000c dcb8 add r6, r4, r5     // r6 = 0xffff
000e 4ee1 addi r7, r6, 1     // r7 = 0
          
0010 0800 nop
0012 0800 nop
          
0014 c040 lbi r0, 0x40       // r0 = 0x0040
0016 c201 lbi r2, 0x1        // r2 = 0x0001
0018 da11 sub r4, r2, r0     // r4 = 0x003f
001a c1e8 lbi r1, 0xe8       // r1 = 0xffe8
001c 41c8 subi r6, r1, 8     // r6 = 0x0020
001e dcd5 sub r5, r4, r6     // r5 = 0xffe1
          
0020 90f0 slbi r0, 0xf0      // r0 = 0x40f0
0022 900f slbi r0, 0x0f      // r0 = 0xf00f
0024 c155 lbi r1, 0x55       // r1 = 0x0055
0026 91aa slbi r1, 0xaa      // r1 = 0x55aa
0028 d82a xor r2, r0, r1     // r2 = 0xa5a5
002a 5ac3 xori r6, r2, 3     // r6 = 0xa5a6
002c d82b andn r2, r0, r1    // r2 = 0xa005
002e 5221 andni r1, r2, 1    // r1 = 0xa004
          
          // Final checksum on the above answers:
0030 db2c add r3, r3, r1     // r3 = 0xa00b
0032 df6c add r3, r7, r3     // r3 = 0xa00b
0034 dbac add r3, r3, r5     // r3 = 0x9fec
0036 dbdc add r7, r3, r6     // r7 = 0x4592
          
          // Some more adder tests
          
0038 c012 lbi r0, 0x12       //
003a 9034 slbi r0, 0x34      // r0 = 0x1234
003c c156 lbi r1, 0x56       //
003e 9178 slbi r1, 0x78      // r1 = 0x5678
0040 c2ab lbi r2, 0xab       //
0042 92cd slbi r2, 0xcd      // r2 = 0xabcd
0044 d82c add r3, r0, r1     // r3 = 0x68ac
0046 d850 add r4, r0, r2     // r4 = 0xbe01
0048 d954 add r5, r1, r2     // r5 = 0x0245
004a db58 add r6, r3, r2     // r6 = 0x1479
004c dc28 add r2, r4, r1     // r2 = 0x1479
004e dd04 add r1, r5, r0     // r1 = 0x1479
0050 d940 add r0, r1, r2     // r0 = 0x28f2
0052 d8c0 add r0, r0, r6     // r0 = 0x3d6b
0054 d8fc add r7, r0, r7     // r7 = 0x82fd
          
0056 0000 halt
0058 0000 halt
005a 0000 halt
005c 0000 halt
