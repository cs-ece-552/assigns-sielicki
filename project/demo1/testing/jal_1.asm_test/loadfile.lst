          //Test of coomon case of calling a subroutine and returning
          //back. Final value of r7 should be zero
0000 c700 lbi r7, 0x0
0002 3006 jal 0x6
0004 47ee subi r7,  r7, 0xe
0006 0800 nop
0008 0000 halt
000a      .label1:		
000a c105 lbi r1, 0x5
000c 37f6 jal -10
000e 37fe jal -2 // this should never be executed or infinite loop will result
          
