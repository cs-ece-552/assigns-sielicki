          // BGEZ assembly test
          //
          // This is a test of the bgez instruction.  This test should simply verify that
          // the branch branches when the register has the value -1 (tests to make sure a
          // value just < 0 doesn't branch).
          
0000 c100 LBI	r1, 0			// loads all 0's into r1 to clear it
0002 493f ADDI	r1, r1, -1		// r1 = -1
0004 7906 BGEZ	r1, .shouldNotGoHere	// -1 !=> 0, so shouldn't branch
          				// all of these should execute
0006 c200 LBI	r2, 0		
0008 4a41 ADDI	r2, r2, 1
000a 4a41 ADDI	r2, r2, 1				
000c      .shouldNotGoHere:
000c 492a ADDI	r1, r1, 10
000e 0000 HALT				// end bgez_4.asm
          				// r1 = 9 (base 10) now
          				// r2 = 2 (base 10) now
