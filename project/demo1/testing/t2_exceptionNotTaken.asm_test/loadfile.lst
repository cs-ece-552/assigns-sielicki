0000 2010 j	.start
0002 1800 rti			// shouldn't be executed
0004 c000 lbi	r0, 0		// none of these are executed
0006 c7ba lbi	r7, 186			
0008 b7e8 rori	r7,  r7, 8	
000a 90dd slbi	r0, 221		
000c df1e xor	r7, r7, r0	
000e 1800 rti
0010 0000 halt	
          
0012      .start:
0012 c100 lbi r1, 0
0014 c001 lbi r0, 1		// r0 = 0x0001
0016 c709 lbi r7, 9		// r7 = 0x0009
0018 0000 halt
001a 0800 nop			// these shouldn't execute
001c 0800 nop
