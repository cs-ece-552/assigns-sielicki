          // SLT
          // if(Rs < Rt) then Rd <- 1 else Rd <-0
          
          // Load Rs with a # smaller than 0xffff
0000 c10a lbi  r1, 0x0a
0002 910a slbi r1, 0x0a
          // Load Rt with a # = to Rs
0004 c20a lbi  r2, 0x0a
0006 920a slbi r2, 0x0a
          // SLT with (Rs = Rt): should return 0
0008 e94c SLT r3, r1, r2
000a 0000 halt
