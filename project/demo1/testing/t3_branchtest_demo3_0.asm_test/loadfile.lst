          //branchtest_demo3_0
          //test all sorts of weird branch conditions
          //test a lot of jumps in a row
          //test branch not taken, branch taken, load
          //test branch not taken, branch taken, non-stall instruction, load
          //test branch not taken, branch taken, branch
          //test branch not taken, branch taken, non-stall instruction, branch
          //test branch not taken, branch taken, branch, load
          //test branch not taken, branch not taken, branch taken, halt
          //test branch not taken, branch not taken, branch taken, non-stall instruction, halt
          //test branch not taken, branch not taken, branch taken, jump
          //test branch not taken, branch not taken, branch taken, non-stall instruction, jump
          
          
          //													pc in hex	
0000      .start:
0000 c00a LBI 	r0, 10				//								pc = 0
0002 c108 LBI	r1, 8				//								pc = 2
          
          
          //test branch not taken, branch not taken, branch taken, jump
0004      .loop:
0004 481f ADDI	r0, r0, -1			//								pc = 4
0006 7080 BLTZ	r0, .error			//never taken							pc = 6
0008 6806 BEQZ	r0, .afterloop			//first several times not taken					pc = 8
000a 79f8 BGEZ	r1, .loop			//always taken							pc = a
000c 207a J	.error				//never taken							pc = c
000e 0000 halt					//								pc = e
          
          
0010      .afterloop:
0010 c007 LBI	r0, 7				//								pc = 10
          
          
          //test branch not taken, branch taken, load
0012      .loop1:
0012 481f ADDI	r0, r0, -1			//								pc = 12
0014 7172 BLTZ	r1, .error			//never taken							pc = 14
0016 78fa BGEZ	r0, .loop1			//taken until r0 becomes negative				pc = 16
0018 7004 BLTZ	r0, .afterloop1			//taken the one time we get here				pc = 18
001a 89c0 LD	r6, r1, 0			//should never happen						pc = 1a
001c 0000 halt					//								pc = 1c
          
          
001e      .afterloop1:
001e c004 LBI 	r0, 4				//								pc = 1e
          //test branch not taken, branch taken, non-stall instruction, load
0020 7066 BLTZ 	r0, .error				//never taken						pc = 20
0022 7804 BGEZ 	r0, .skip_ahead0			//taken							pc = 22
0024 4802 ADDI 	r0, r0, 2				//r0 better not get 6					pc = 24
0026 89c0 LD	r6, r1, 0				//should never happen					pc = 26 
          
          
0028      .skip_ahead0:
          //test branch not taken, branch taken, branch
0028 c410 LBI	r4, 16					//mark that we are here					pc = 28
002a 705c BLTZ 	r0, .error				//never taken						pc = 2a
002c 7804 BGEZ 	r0, .skip_ahead1			//taken							pc = 2c
002e 7858 BGEZ	r0, .error				//shouldn't happen					pc = 2e
0030 7856 BGEZ	r0, .error				//shouldn't happen					pc = 30
          
0032      .skip_ahead1:
          //test branch not taken, branch taken, non-stall instruction, branch
0032 c411 LBI	r4, 17					//mark that we are here					pc = 32
0034 7052 BLTZ 	r0, .error				//never taken						pc = 34
0036 7804 BGEZ 	r0, .skip_ahead2			//taken							pc = 36
0038 4802 ADDI	r0, r0, 2				//r0 better stay 4					pc = 38
003a 784c BGEZ	r0, .error				//shouldn't happen					pc = 3a
          
003c      .skip_ahead2:
          //test branch not taken, branch taken, branch, load
003c c412 LBI	r4, 18					//mark that we are here					pc = 3c
003e 7048 BLTZ 	r0, .error				//never taken						pc = 3e
0040 7804 BGEZ 	r0, .skip_ahead3			//taken							pc = 40
0042 7844 BGEZ	r0, .error				//shouldn't happen					pc = 42
0044 89c0 LD	r6, r1, 0				//shouldn't happen					pc = 44
          
0046      .skip_ahead3:
          //test branch not taken, branch not taken, branch taken, halt
0046 c413 LBI	r4, 19					//mark that we are here					pc = 46
0048 703e BLTZ	r0, .error				//never taken						pc = 48
004a 703c BLTZ 	r0, .error				//never taken						pc = 4a
004c 7804 BGEZ 	r0, .skip_ahead4			//taken							pc = 4c
004e 0000 halt						//							pc = 4e
0050 0000 halt						//							pc = 50
          
0052      .skip_ahead4:
          //test branch not taken, branch not taken, branch taken, non-stall instruction, halt
0052 c414 LBI	r4, 20					//mark that we are here					pc = 52
0054 7032 BLTZ	r0, .error				//never taken						pc = 54
0056 7030 BLTZ 	r0, .error				//never taken						pc = 56
0058 7804 BGEZ 	r0, .skip_ahead5			//taken							pc = 58
005a 4802 ADDI	r0, r0, 2				//r0 better stay 4					pc = 5a
005c 0000 halt						//							pc = 5c
          
005e      .skip_ahead5:
          //test branch not taken, branch not taken, branch taken, jump
005e c415 LBI	r4, 21					//mark that we are here					pc = 5e
0060 7026 BLTZ	r0, .error				//never taken						pc = 60
0062 7024 BLTZ 	r0, .error				//never taken						pc = 62
0064 7804 BGEZ 	r0, .skip_ahead6			//taken							pc = 64
0066 3020 JAL	.error					//shouldn't happen					pc = 66
0068 0000 halt						//							pc = 68
          
006a      .skip_ahead6:
          //test branch not taken, branch not taken, branch taken, non-stall instruction, jump
006a c416 LBI	r4, 22					//mark that we are here					pc = 6a
006c 701a BLTZ	r0, .error				//never taken						pc = 6c
006e 7018 BLTZ 	r0, .error				//never taken						pc = 6e
0070 7804 BGEZ 	r0, .skip_ahead7			//taken							pc = 70
0072 4802 ADDI	r0, r0, 2				//r0 better stay 4					pc = 72
0074 3012 JAL	.error					//shouldn't happen					pc = 74
          
0076      .skip_ahead7:
0076 c417 LBI	r4, 23					//mark that we are here					pc = 76
0078 2006 J	.exit					//							pc = 78
007a 200c J	.error					//							pc = 7a
007c 200a J	.error					//							pc = 7c
007e 2008 J	.error					//							pc = 7e
          
0080      .exit:
0080 c002 LBI 	r0, 2					//							pc = 80
0082 0000 halt						//DONE DONE DONE					pc = 82
0084 2002 J	.error					//							pc = 84
0086 2000 J	.error					//							pc = 86
          
          
0088      .error:
0088 c206 LBI	r2, 6					//better not get here					pc = 88
008a 0000 halt						//							pc = 8a
008c 0000 halt						//							pc = 8c
008e 0000 halt						//							pc = 8e
0090 0000 halt						//							pc = 90
