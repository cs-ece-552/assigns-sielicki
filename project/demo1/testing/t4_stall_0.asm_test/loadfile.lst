          //This program tests if JALR works correctly with data memory stall
0000 2002 j .start 
0002 000e data 14  //jump location
          
0004      .start:
0004 c002 lbi r0, 2   //address of jump location
0006 88e0 ld r7, r0, 0 //load jump address to R7
0008 3f00 jalr r7, 0 //Jump to address pointed by r7, hazard detect unit should detect this and cause pipeline stall
000a c001 lbi r0, 1
000c 0000 halt
000e 3f00 jalr r7, 0  //first JALR should jump here
